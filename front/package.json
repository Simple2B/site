{
  "name": "front",
  "version": "1.3.20",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "gApi": "openapi --input http://127.0.0.1:5002/openapi.json --output ./src/openapi --client axios",
    "js-watch": "webpack --config webpack.dev.js --watch",
    "js": "webpack --config webpack.prod.js",
    "css": "tailwindcss -i ./src/app/styles/globals.css -o ./build/static/css/styles.css --minify",
    "css-watch": "tailwindcss -i ./src/app/styles/globals.css -o ./build/static/css/styles.css --watch",
    "build": "yarn css && next build",
    "test": "vitest --run"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/react-google-recaptcha": "^2.1.5",
    "@vitejs/plugin-react": "^4.0.3",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "next-auth": "^4.22.1",
    "openapi-typescript-codegen": "^0.24.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.44.3",
    "react-responsive": "^9.0.2",
    "react-spinners": "^0.13.8",
    "sass": "^1.62.1",
    "server-only": "^0.0.1",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "vitest": "^0.33.0"
  }
}
